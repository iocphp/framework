{
    "name": "iocphp/framework",
    "type": "library",
    "description": "iocphp framework",
    "license": "MIT",    
    "authors": [
        {
            "name": "caocao",
            "email": "pgcao@qq.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/error-handler": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/runtime": "^5.4",
        "mpociot/pipeline": "^1.0",
        "psr/container": "^1.0",
        "filp/whoops": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "iocphp\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "minimum-stability": "dev"
}
